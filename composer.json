{
  "name": "lessname/cache",
  "description": "Cache your values",
  "license": "proprietary",
  "require": {
    "php": "^8.1.0",

    "psr/simple-cache": "^3.0.0",
    "psr/container": "^1.0.0 || ^2.0.0"
  },
  "require-dev": {
    "ext-redis": "*",

    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^9.5.0",
    "vimeo/psalm": "^4.10.0"
  },
  "autoload": {
    "psr-4": {
      "LessCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessCacheTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
